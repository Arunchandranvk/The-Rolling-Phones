{
    "builds": [
      {
        "src": "onlinemobilestore/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        },
        "command": "./build.sh"  
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "onlinemobilestore/wsgi.py"
      }
    ]
  }
  