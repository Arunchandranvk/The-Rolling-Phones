
  
  {
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/manage.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "onlinemobilestore.settings"
    },
    "build": {
      "env": {
        "DJANGO_SETTINGS_MODULE": "onlinemobilestore.settings"
      },
      "command": "python manage.py migrate"
    }
  }
  