{
    "builds": [
      {
        "src": "onlinemobilestore/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "onlinemobilestore/wsgi.py"
      }
    ],
    "command": "./build.sh" 
  }