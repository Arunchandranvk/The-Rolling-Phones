{
    "builds": [
      {
        "src": "onlinemobilestore/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        },
        "functions": {  
          "build": {
            "runtime": "python3.9",
            "memory": 1024,
            "includeFiles": ["./build.sh"],  
            "sh": "./build.sh"  
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "onlinemobilestore/wsgi.py"
      }
    ]
  }
  